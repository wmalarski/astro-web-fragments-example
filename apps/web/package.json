{
  "dependencies": {
    "@awfe/paths": "workspace:*",
    "@opennextjs/cloudflare": "^1.11.1",
    "@tailwindcss/postcss": "4.1.17",
    "next": "^15.5.6",
    "postcss": "8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "4.1.17",
    "web-fragments": "^0.8.2"
  },
  "devDependencies": {
    "@awfe/tailwind-config": "workspace:*",
    "@awfe/typescript-config": "workspace:*",
    "@cloudflare/workers-types": "^4.20251109.0",
    "@types/node": "^24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "typescript": "5.9.3",
    "wrangler": "^4.46.0"
  },
  "name": "@awfe/web",
  "private": true,
  "scripts": {
    "build": "next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "check-types": "next typegen && tsc --noEmit",
    "clear": "rm -rf node_modules .turbo .open-next .next .wrangler",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "dev": "next dev --port 3002",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "start": "next start"
  },
  "type": "module",
  "version": "0.1.0"
}
